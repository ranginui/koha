[% INCLUDE 'doc-head-open.inc' %][% IF ( LibraryNameTitle ) %][% LibraryNameTitle %][% ELSE %]Koha Online[% END %] Catalogue &rsaquo; Entry [% INCLUDE 'doc-head-close.inc' %] [% IF ( displayhierarchy ) %] <link rel="stylesheet" type="text/css" href="[% themelang %]/css/hierarchy.css">
<script language="JavaScript" type="text/javascript">
function showParents(mynumber) {
  var parents=document.getElementsByName(mynumber+'p')
  for(i=0;i<parents.length;i++){
    if (parents[i].style.display == "none") {
      parents[i].style.display ="block";
    } else {
      parents[i].style.display ="none";
    }
  } 
}
function showChildren(mynumber) {
  var children=document.getElementsByName(mynumber+'c')
  for(i=0;i<children.length;i++){
    if (children[i].style.display == "none") {
      children[i].style.display = "block";
    } else {
      children[i].style.display = "none";
    }
  }
}
</script>
[% END %] </head>
