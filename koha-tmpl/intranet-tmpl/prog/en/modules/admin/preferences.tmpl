    <!-- TMPL_IF NAME="jump_not_found" -->
    <h2>System preferences</h2>
    <div class="dialog error">
        Could not find a system preference named <code><!-- TMPL_VAR NAME="jumpfield" --></code>.
    </div>
    <!-- /TMPL_IF -->
    <!-- TMPL_IF NAME="search_not_found" -->
