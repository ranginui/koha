<script>

// Pad the string with spaces so that it is 4 characters long.
// Also, truncate strings that are longer than 4 characters.
function pad4(s) {
	var pad = "";
	if (s.length < 4) {
		var padLength = 4 - s.length;
		for (var i = 0; i < 4 - s.length; i++) {
			pad += " ";
		}
		s += pad;
	} else {
		s = s.substring(0, 4);
	}
	return s;
}

function report() {
